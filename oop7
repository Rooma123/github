#override
class Person :
    def __init__(self,name , age):
        self.name=name
        self.age=age

    def display(self):
        print( f"my name is {self.name } and my age is {self.age}")
        print("function inside parent")



class Student(Person):
    def __init__(self,name,age,grades):
        super().__init__(name,age)
        self.grades=grades

    def display(self):
        print(f"my name is {self.name} and my age is {self.age}")
        print("function inside inheret")

person1=Person("Reem",20)
student1=Student("rrrr",15,90)
person1.display()
student1.display()